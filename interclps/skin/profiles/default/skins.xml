<?xml version="1.0"?>

<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="interclps">

 <object name="interclps_skin_custom_images"
    meta_type="Filesystem Directory View"
    directory="interclps.skin:skins/interclps_skin_images"/>
 <object name="interclps_skin_custom_templates"
    meta_type="Filesystem Directory View"
    directory="interclps.skin:skins/interclps_skin_templates"/>
 <object name="interclps_skin_styles"
    meta_type="Filesystem Directory View"
    directory="interclps.skin:skins/interclps_skin_styles"/>

 <skin-path name="interclps" based-on="Sunburst Theme">
  <layer name="interclps_skin_images"
     insert-after="custom"/>
  <layer name="interclps_skin_templates"
     insert-after="interclps_skin_images"/>
  <layer name="interclps_skin_styles"
     insert-after="interclps_skin_templates"/>
 </skin-path>

</object>
